{
  "name": "moms-serverless",
  "version": "1.0.0",
  "description": "Modular Organizational Management System - Serverless",
  "scripts": {
    "deploy": "serverless deploy",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "logs": "serverless logs -f standardApi -t",
    "migrate": "serverless invoke -f migrations",
    "collectstatic": "serverless invoke -f collectstatic"
  },
  "devDependencies": {
    "serverless": "^3.30.1",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-python-requirements": "^6.0.0",
    "serverless-wsgi": "^3.0.2"
  }
}